{
  "name": "domaine",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "dev": "astro dev --host",
    "dev:content": "SERVER_RENDERING_ENABLED='true' astro build && wrangler pages dev ./dist",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' astro build && pagefind --site dist  && cp -r dist/pagefind public/ && echo '_worker.js\n_routes.json' > .assetsignore && cp .assetsignore dist/",
    "build-ssr": "NODE_OPTIONS='--max-old-space-size=4096' SERVER_RENDERING_ENABLED='true' astro build && cp public/.assetsignore dist/",
    "index": "pagefind --site dist  && cp -r dist/pagefind public/",
    "start": "astro dev",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.5.3",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.0",
    "@astrojs/solid-js": "^5.1.0",
    "@mux/mux-player": "^2.7.0",
    "@sanity/astro": "^3.2.6",
    "@sanity/client": "^6.29.1",
    "@sanity/color-input": "^3.1.1",
    "@sanity/image-url": "^1.0.2",
    "@sanity/orderable-document-list": "^1.2.1",
    "@sanity/table": "^1.1.2",
    "@sanity/visual-editing": "^2.1.10",
    "@studio-freight/lenis": "^1.0.37",
    "@types/lodash": "^4.17.14",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "astro": "^5.8.0",
    "astro-icon": "^1.1.0",
    "astro-portabletext": "^0.9.9",
    "astro-sanity-picture": "^0.2.0",
    "gsap": "^3.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-is": "^19.1.0",
    "sanity": "^3.89.0",
    "sanity-plugin-media": "^2.2.5",
    "sanity-plugin-mux-input": "^2.3.4",
    "sanity-plugin-vercel-deploy": "^3.3.4",
    "sanity-plugin-webhooks-trigger": "^0.3.1",
    "solid-devtools": "^0.30.1",
    "solid-js": "^1.8.17",
    "styled-components": "^6.1.18",
    "swiper": "^11.1.8",
    "three": "^0.167.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "pagefind": "^1.1.0",
    "wrangler": "^4.17.0"
  }
}

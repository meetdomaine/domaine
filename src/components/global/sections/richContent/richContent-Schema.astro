---
const { node } = Astro.props;
const string = JSON.stringify(node.markup);
const markup = JSON.parse(string);
---
<script define:vars={{ markup }}>
  const markupScript = document.createElement("script");
  markupScript.type = "application/ld+json";
  markupScript.innerText = JSON.stringify(markup);
  document.head.appendChild(markupScript);
</script>
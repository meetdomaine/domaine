---
import Form from "./section-Form.astro"
import ProjectsFeed from "./section-ProjectsFeed.astro"
import BlogFeed from "./section-BlogFeed.astro"
import TextMedia from "./section-TextMedia.astro"
import VideoPlayer from "./section-VideoPlayer.astro"
import ServicesFeed from "./section-ServicesFeed.astro"
import ContentBlocks from "./section-ContentBlocks.astro"
import ProjectsFullBleed from "./section-ProjectsFullBleed.astro"
import TextLinkCard from "./section-TextLinkCard.astro"
import ProjectsGrid from "./section-ProjectsGrid.astro"
import PartnersFeed from "./section-PartnersFeed.astro"
import TextClients from "./section-TextClients.astro"
import ImageFullHeight from "./section-ImageFullHeight.astro"
import TextColumns from "./section-TextColumns.astro"
import MediaCarousel from "./section-MediaCarousel.astro"
import PracticesFeed from "./section-PracticesFeed.astro"
import TextVideoPlayer from "./section-TextVideoPlayer.astro"
import TextMediaTabs from "./section-TextMediaTabs.astro"
import ServiceFeature from "./section-ServiceFeature.astro"
import StatsCarousel from "./section-StatsCarousel.astro"
import ServiceCards from "./section-ServiceCards.astro"
import LinkList from "./section-LinkList.astro"
import Quote from "./section-Quote.astro"
import RichContent from "./section-RichContent.astro"
import TextHeading from "./section-TextHeading.astro"
import LocationsFeed from "./section-LocationsFeed.astro"
import TextMediaBlocks from "./section-TextMediaBlocks.astro"
import SectionMediaFullbleed from "./section-MediaFullbleed.astro"
import Agenda from "./section-Agenda.astro"

const { section, brand, locale, key } = Astro.props
---
{section._type == "section_agenda" && section.showSection && <Agenda section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_blogFeed" && section.showSection && <BlogFeed section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_contentBlocks" && section.showSection && <ContentBlocks section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_form" && section.showSection && <Form section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_imageFullHeight" && section.showSection && <ImageFullHeight section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_locationsFeed" && section.showSection && <LocationsFeed section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_linkList" && section.showSection && <LinkList section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_mediaCarousel" && section.showSection && <MediaCarousel section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_mediaFullbleed" && section.showSection && <SectionMediaFullbleed section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_partnersFeed" && section.showSection && <PartnersFeed section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_practicesFeed" && section.showSection && <PracticesFeed section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_projectsFeed" && section.showSection && <ProjectsFeed section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_projectsFullBleed" && section.showSection && <ProjectsFullBleed section={section} brand={brand} locale={locale} key={key} />}
<!-- {section._type == "section_projectsGrid" && section.showSection && <ProjectsGrid section={section} brand={brand} locale={locale} key={key} />} -->
{section._type == "section_quote" && section.showSection && <Quote section={section} brand={brand} locale={locale} key={key} />}
<!-- {section._type == "section_richContent" && section.showSection && <RichContent section={section} brand={brand} locale={locale} key={key} />} -->
{section._type == "section_serviceCards" && section.showSection && <ServiceCards section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_serviceFeature" && section.showSection && <ServiceFeature section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_servicesFeed" && section.showSection && <ServicesFeed section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_statsCarousel" && section.showSection && <StatsCarousel section={section} brand={brand} locale={locale} key={key} />}
<!-- {section._type == "section_textClients" && section.showSection && <TextClients section={section} brand={brand} locale={locale} key={key} />} -->
{section._type == "section_textColumns" && section.showSection && <TextColumns section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_textHeading" && section.showSection && <TextHeading section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_textLinkCard" && section.showSection &&  <TextLinkCard section={section} brand={brand} locale={locale} key={key} />}
<!-- {section._type == "section_textMedia" && section.showSection && <TextMedia section={section} brand={brand} locale={locale} key={key} />} -->
{section._type == "section_textMediaBlocks" && section.showSection && <TextMediaBlocks section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_textMediaTabs" && section.showSection && <TextMediaTabs section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_textVideoPlayer" && section.showSection && <TextVideoPlayer section={section} brand={brand} locale={locale} key={key} />}
{section._type == "section_videoPlayer" && section.showSection && <VideoPlayer section={section} brand={brand} locale={locale} key={key} />}



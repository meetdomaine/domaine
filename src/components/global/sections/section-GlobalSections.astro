---
import Form from "./section-Form.astro"
import ProjectsFeed from "./section-ProjectsFeed.astro"
import BlogFeed from "./section-BlogFeed.astro"
import TextMedia from "./section-TextMedia.astro"
import VideoPlayer from "./section-VideoPlayer.astro"
import ServicesFeed from "./section-ServicesFeed.astro"
import ContentBlocks from "./section-ContentBlocks.astro"
import ProjectsFullBleed from "./section-ProjectsFullBleed.astro"
import HomeAbout from "../../domaine/sections/home/home-About.astro"
import ProjectsGrid from "./section-ProjectsGrid.astro"
import PartnersFeed from "./section-PartnersFeed.astro"

const { section, brand, key } = Astro.props
---
{section._type == "section_form" && <Form section={section} brand={brand} key={key} />}
{section._type == "section_projectsFeed" && <ProjectsFeed section={section} brand={brand} key={key} />}
{section._type == "section_projectsFullBleed" && <ProjectsFullBleed section={section} brand={brand} key={key} />}
{section._type == "section_projectsGrid" && <ProjectsGrid section={section} brand={brand} key={key} />}
{section._type == "section_blogFeed" && <BlogFeed section={section} brand={brand} key={key} />}
{section._type == "section_textMedia" && <TextMedia section={section} brand={brand} key={key} />}
{section._type == "section_videoPlayer" && <VideoPlayer section={section} brand={brand} key={key} />}
{section._type == "section_servicesFeed" && <ServicesFeed section={section} brand={brand} key={key} />}
{section._type == "section_contentBlocks" && <ContentBlocks section={section} brand={brand} key={key} />}
{section._type == "section_domaine-about" && <HomeAbout section={section} brand={brand} key={key} />}
{section._type == "section_partnersFeed" && <PartnersFeed section={section} brand={brand} key={key} />}


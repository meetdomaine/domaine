---
import Form from "./section-Form.astro"
import ProjectsFeed from "./section-ProjectsFeed.astro"
import BlogFeed from "./section-BlogFeed.astro"
import TextMedia from "./section-TextMedia.astro"
import VideoPlayer from "./section-VideoPlayer.astro"
import ServicesFeed from "./section-ServicesFeed.astro"
import ContentBlocks from "./section-ContentBlocks.astro"
import ProjectsFullBleed from "./section-ProjectsFullBleed.astro"
import TextFeed from "./section-TextFeed.astro"
import ProjectsGrid from "./section-ProjectsGrid.astro"
import PartnersFeed from "./section-PartnersFeed.astro"
import TextClients from "./section-TextClients.astro"
import ImageFullHeight from "../projects/workPost-ImageFullHeight.astro"
import TextColumns from "./section-TextColumns.astro"
import MediaCarousel from "./section-MediaCarousel.astro"
import PracticesFeed from "./section-PracticesFeed.astro"
import TextVideoPlayer from "./section-TextVideoPlayer.astro"
import TextMediaTabs from "./section-TextMediaTabs.astro"
import ServiceType from "./section-ServiceType.astro"

const { section, brand, key } = Astro.props
---
{section._type == "section_form" && section.showSection && <Form section={section} brand={brand} key={key} />}
{section._type == "section_projectsFeed" && section.showSection && <ProjectsFeed section={section} brand={brand} key={key} />}
{section._type == "section_projectsFullBleed" && section.showSection && <ProjectsFullBleed section={section} brand={brand} key={key} />}
{section._type == "section_projectsGrid" && section.showSection && <ProjectsGrid section={section} brand={brand} key={key} />}
{section._type == "section_blogFeed" && section.showSection && <BlogFeed section={section} brand={brand} key={key} />}
{section._type == "section_textColumns" && section.showSection && <TextColumns section={section} brand={brand} key={key} />}
{section._type == "section_textMedia" && section.showSection && <TextMedia section={section} brand={brand} key={key} />}
{section._type == "section_textClients" && section.showSection && <TextClients section={section} brand={brand} key={key} />}
{section._type == "section_videoPlayer" && section.showSection && <VideoPlayer section={section} brand={brand} key={key} />}
{section._type == "section_servicesFeed" && section.showSection && <ServicesFeed section={section} brand={brand} key={key} />}
{section._type == "section_contentBlocks" && section.showSection && <ContentBlocks section={section} brand={brand} key={key} />}
{section._type == "section_textFeed" && section.showSection &&  <TextFeed section={section} brand={brand} key={key} />}
{section._type == "section_partnersFeed" && section.showSection && <PartnersFeed section={section} brand={brand} key={key} />}
{section._type == "section_imageFullHeight" && section.showSection && <ImageFullHeight section={section} brand={brand} key={key} />}
{section._type == "section_mediaCarousel" && section.showSection && <MediaCarousel section={section} brand={brand} key={key} />}
{section._type == "section_practicesFeed" && section.showSection && <PracticesFeed section={section} brand={brand} key={key} />}
{section._type == "section_textVideoPlayer" && section.showSection && <TextVideoPlayer section={section} brand={brand} key={key} />}
{section._type == "section_textMediaTabs" && section.showSection && <TextMediaTabs section={section} brand={brand} key={key} />}
{section._type == "section_serviceType" && section.showSection && <ServiceType section={section} brand={brand} key={key} />}


---
// STATICALLY GENERATED AT BUILD TIME
import { Brands } from '../../enums/brands';
import LayoutProjectsIndex from '../../layouts/Layout_ProjectsIndex.astro';
import { getCollection } from 'astro:content';

export const prerender = true

const allProjects = await getCollection('projectCards')
const domaineProjects = allProjects.filter(project => project.data.agencyBrand.name === Brands.DOMAINE)
---
<LayoutProjectsIndex
    brand={Brands.DOMAINE}
    projects={domaineProjects.map(project => project.data)}
    isTopLevel
/>
